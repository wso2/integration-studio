		 <artifact.priority
      artifactType="service/dataservice"
      priority="5"/>
		 <artifact.priority
      artifactType="cep/bucket"
      priority="5"/>
		 <artifact.priority
      artifactType="carbon/application"
      priority="5"/>
		 <artifact.priority
