<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
    <modelVersion>4.0.0</modelVersion>
    <groupId>org.wso2.integrationstudio</groupId>
    <artifactId>wso2-integration-studio-parent</artifactId>
    <version>8.0.0-SNAPSHOT</version>
    <packaging>pom</packaging>
    <name>WSO2 Integration Studio - Parent</name>
    <description>WSO2 Integration Studio</description>

    <scm>
        <connection>scm:git:https://github.com/wso2/integration-studio.git</connection>
        <developerConnection>scm:git:https://github.com/wso2/integration-studio.git</developerConnection>
        <url>https://github.com/wso2/integration-studio.git</url>
    </scm>

    <distributionManagement>
        <repository>
            <id>nexus-releases</id>
            <name>WSO2 Release Distribution Repository</name>
            <url>https://maven.wso2.org/nexus/service/local/staging/deploy/maven2/</url>
        </repository>
    </distributionManagement>

    <profiles>
        <profile>
            <id>all-components</id>
            <modules>
                <module>components/studio-platform</module>
                <module>components/esb-tools</module>
                <module>components/dss-tools</module>
                <module>components/bps-tools</module>
            </modules>
        </profile>
        <profile>
            <id>studio-platform</id>
            <modules>
                <module>components/studio-platform</module>
            </modules>
        </profile>
        <profile>
            <id>esb-tools</id>
            <modules>
                <module>components/studio-platform</module>
                <module>components/esb-tools</module>
            </modules>
        </profile>
        <profile>
            <id>dss-tools</id>
            <modules>
                <module>components/studio-platform</module>
                <module>components/dss-tools</module>
            </modules>
        </profile>
        <profile>
            <id>bps-tools</id>
            <modules>
                <module>components/studio-platform</module>
                <module>components/bps-tools</module>
            </modules>
        </profile>
        <profile>
            <id>server-tools</id>
            <modules>
                <module>components/studio-platform</module>
                <module>components/sever-tools</module>
            </modules>
        </profile>
        <profile>
            <activation>
                <activeByDefault>true</activeByDefault>
            </activation>
            <modules>
                <module>components</module>
                <module>distribution</module>
            </modules>
        </profile>
    </profiles>

    <dependencyManagement>
        <dependencies>
            <dependency>
                <groupId>com.google.code.gson</groupId>
                <artifactId>gson</artifactId>
                <version>${com.google.code.gson}</version>
            </dependency>
            <dependency>
                <groupId>commons-io.wso2</groupId>
                <artifactId>commons-io</artifactId>
                <version>${commons-io.wso2}</version>
            </dependency>
            <dependency>
                <groupId>org.apache.ws.commons.axiom.wso2</groupId>
                <artifactId>axiom</artifactId>
                <version>${org.apache.ws.commons.axiom.wso2}</version>
            </dependency>
            <dependency>
                <groupId>wsdl4j.wso2</groupId>
                <artifactId>wsdl4j</artifactId>
                <version>${wsdl4j.wso2}</version>
            </dependency>
            <dependency>
                <groupId>org.apache.axis2.wso2</groupId>
                <artifactId>axis2</artifactId>
                <version>${org.apache.axis2.wso2}</version>
            </dependency>
            <dependency>
                <groupId>commons-validator</groupId>
                <artifactId>commons-validator</artifactId>
                <version>${commons.validator}</version>
            </dependency>
            <dependency>
                <groupId>commons-httpclient.wso2</groupId>
                <artifactId>commons-httpclient</artifactId>
                <version>${commons.httpclient.wso2}</version>
            </dependency>
            <dependency>
                <groupId>org.eclipse.jetty</groupId>
                <artifactId>jetty-server</artifactId>
                <version>${org.eclipse.jetty.server}</version>
            </dependency>
            <dependency>
                <groupId>org.eclipse.jetty</groupId>
                <artifactId>jetty-servlet</artifactId>
                <version>${org.eclipse.jetty.servlet}</version>
            </dependency>
            <dependency>
                <groupId>org.codehaus.jettison.wso2</groupId>
                <artifactId>jettison</artifactId>
                <version>${org.codehaus.jettison.wso2}</version>
            </dependency>
            <dependency>
                <groupId>xmlunit</groupId>
                <artifactId>xmlunit</artifactId>
                <version>${xmlunit}</version>
            </dependency>   
            <dependency>
                <groupId>org.apache.xalan</groupId>
                <artifactId>xalan</artifactId>
                <version>${org.apache.xalan}</version>
            </dependency>  
            <dependency>
                <groupId>wsdl4j</groupId>
                <artifactId>wsdl4j</artifactId>
                <version>${wsdl4j}</version>
            </dependency>
            <dependency>
                <groupId>org.apache.tomcat</groupId>
                <artifactId>tribes</artifactId>
                <version>${org.apache.tomcat.tribes}</version>
            </dependency>
            <dependency>
                <groupId>org.codehaus.woodstox</groupId>
                <artifactId>wstx-asl</artifactId>
                <version>${org.codehaus.woodstox.wstx.asl}</version>
            </dependency>
            <dependency>
                <groupId>javax.xml.stream</groupId>
                <artifactId>stax-api</artifactId>
                <version>${javax.xml.stream.stax.api}</version>
            </dependency> 
            <dependency>
                <groupId>org.codehaus.jettison</groupId>
                <artifactId>jettison</artifactId>
                <version>${org.codehaus.jettison}</version>
            </dependency>
            <dependency>
                <groupId>org.activiti.designer</groupId>
                <artifactId>org.activiti.designer.integration</artifactId>
                <version>${activti.designer.version}</version>
            </dependency>
        </dependencies>
    </dependencyManagement>

