        <commons.validator>1.4.0</commons.validator>
        <commons.httpclient.wso2>3.1.0.wso2v2</commons.httpclient.wso2>
        <org.codehaus.mojo.buildnumber.maven.plugin>1.2</org.codehaus.mojo.buildnumber.maven.plugin>
